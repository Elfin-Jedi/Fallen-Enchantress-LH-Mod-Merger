<?xml version="1.0" encoding="iso-8859-1"?>
<ImprovementTypes>
	
	<ImprovementType InternalName="Chapel">
        <DisplayName>Chapel</DisplayName>
        <Description>Chapels further the cause of the faith, increasing Growth and Mana. Each Chapel houses a small number of soldiers from the Order of the Phoenix Shield.</Description>
        <RequiresCity>1</RequiresCity>
        <BarredTerrain>River</BarredTerrain>
        <BarredTerrain>SwampTerrain</BarredTerrain>
        <PreferredTerrain>City</PreferredTerrain>
        <PreferredTerrain>Category:Land</PreferredTerrain>
        <PreferredTerrain>Forest</PreferredTerrain>
        <RequiresCityImprovement>1</RequiresCityImprovement>
        <RequiredImprovement>TradeGuild</RequiredImprovement>
        <UpgradesToImprovement>Church</UpgradesToImprovement>
        <UpgradeTier>2</UpgradeTier>
        <Prereq>
            <Type>Tech</Type>
            <Attribute>Civics</Attribute>
        </Prereq>
		<Prereq>
            <Type>AbilityBonusOption</Type>
            <Attribute>HolyCrusaders</Attribute>
            <Target>Player</Target>
        </Prereq>
        <LaborToBuild>249</LaborToBuild>
        <GameModifier>
            <ModType>ResourceMultiplier</ModType>
            <Attribute>Food</Attribute>
            <Value>1.5</Value>
            <PerTurn>1</PerTurn>
            <Provides>+50% Food</Provides>
            <UpgradeComparisonID>1</UpgradeComparisonID>
        </GameModifier>
        <GameModifier>
            <ModType>Resource</ModType>
            <Attribute>Mana</Attribute>
            <Value>1</Value>
            <PerTurn>1</PerTurn>
            <Provides>+1 Mana</Provides>
            <UpgradeComparisonID>1</UpgradeComparisonID>
        </GameModifier>
		<GameModifier>
            <ModType>Player</ModType>
            <Attribute>LimitedUnitCap</Attribute>
            <StrVal>Crusader</StrVal>
            <Value>1</Value>
            <Provides>Musters a Crusader</Provides>
        </GameModifier>
        <AIData AIPersonality="AI_General">
            <AIPriority>10</AIPriority>
            <AITag>Prestige</AITag>
            <AIEarlyBuildUpMultiplier>15</AIEarlyBuildUpMultiplier>
        </AIData>
        <ArtDef>Art_Chapel</ArtDef>
    </ImprovementType>
	
	<ImprovementType InternalName="Church">
        <DisplayName>Church</DisplayName>
        <Description>Churches attract new citizens to the faith, increasing Growth and Mana. Each Church houses a moderate number of soldiers from the Order of the Phoenix Shield.</Description>
        <RequiresCity>1</RequiresCity>
        <BarredTerrain>River</BarredTerrain>
        <BarredTerrain>SwampTerrain</BarredTerrain>
        <PreferredTerrain>City</PreferredTerrain>
        <PreferredTerrain>Category:Land</PreferredTerrain>
        <PreferredTerrain>Forest</PreferredTerrain>
        <RequiresCityImprovement>1</RequiresCityImprovement>
        <RequiredImprovement>TradeGuild</RequiredImprovement>
        <UpgradesToImprovement>Cathedral</UpgradesToImprovement>
        <UpgradeTier>3</UpgradeTier>
        <Prereq>
            <Type>Tech</Type>
            <Attribute>Administration</Attribute>
        </Prereq>
		<Prereq>
            <Type>AbilityBonusOption</Type>
            <Attribute>HolyCrusaders</Attribute>
            <Target>Player</Target>
        </Prereq>
        <LaborToBuild>249</LaborToBuild>
        <GameModifier>
            <ModType>ResourceMultiplier</ModType>
            <Attribute>Food</Attribute>
            <Value>1.5</Value>
            <PerTurn>1</PerTurn>
            <Provides>+50% Food</Provides>
            <UpgradeComparisonID>1</UpgradeComparisonID>
        </GameModifier>
        <GameModifier>
            <ModType>Resource</ModType>
            <Attribute>Mana</Attribute>
            <Value>2</Value>
            <PerTurn>1</PerTurn>
            <Provides>+2 Mana</Provides>
            <UpgradeComparisonID>1</UpgradeComparisonID>
        </GameModifier>
        <GameModifier>
            <ModType>ResourceMultiplier</ModType>
            <Attribute>Prestige</Attribute>
            <Value>1</Value>
            <PerTurn>1</PerTurn>
            <IsProductionQueueEmpty>1</IsProductionQueueEmpty>
            <Provides>+50% Growth when Producing Growth</Provides>
            <Calculate InternalName="Calc" ValueOwner="OwnerCity">
                <Expression><![CDATA[[Constructing_ProduceGrowth] * 0.5]]></Expression>
            </Calculate>
            <Calculate InternalName="Value" ValueOwner="OwnerCity">
                <Expression><![CDATA[[Calc] + 1]]></Expression>
            </Calculate>
        </GameModifier>
		<GameModifier>
            <ModType>Player</ModType>
            <Attribute>LimitedUnitCap</Attribute>
            <StrVal>Crusader</StrVal>
            <Value>2</Value>
            <Provides>Musters a Crusader</Provides>
        </GameModifier>
        <AIData AIPersonality="AI_General">
            <AIPriority>10</AIPriority>
            <AITag>Prestige</AITag>
            <AIEarlyBuildUpMultiplier>15</AIEarlyBuildUpMultiplier>
        </AIData>
        <ArtDef>Art_Church</ArtDef>
    </ImprovementType>
	
	<ImprovementType InternalName="Cathedral">
        <DisplayName>Cathedral</DisplayName>
        <Description>The Cathedral is a huge bulwark of the faith. It will greatly increase Growth and Mana. The Cathedral houses a large number soldiers from the Order of the Phoenix Shield.</Description>
        <AllowedPerFaction>1</AllowedPerFaction>
        <RequiresCity>1</RequiresCity>
        <BarredTerrain>River</BarredTerrain>
        <BarredTerrain>SwampTerrain</BarredTerrain>
        <PreferredTerrain>City</PreferredTerrain>
        <PreferredTerrain>Category:Land</PreferredTerrain>
        <PreferredTerrain>Forest</PreferredTerrain>
        <RequiresCityImprovement>1</RequiresCityImprovement>
        <RequiredImprovement>TradeGuild</RequiredImprovement>
        <UpgradeTier>4</UpgradeTier>
        <Prereq>
            <Type>Tech</Type>
            <Attribute>Entertainment</Attribute>
        </Prereq>
		<Prereq>
            <Type>AbilityBonusOption</Type>
            <Attribute>HolyCrusaders</Attribute>
            <Target>Player</Target>
        </Prereq>
        <LaborToBuild>1124</LaborToBuild>
        <GameModifier>
            <ModType>ResourceMultiplier</ModType>
            <Attribute>Food</Attribute>
            <Value>1.5</Value>
            <PerTurn>1</PerTurn>
            <Provides>+50% Food</Provides>
            <UpgradeComparisonID>1</UpgradeComparisonID>
        </GameModifier>
        <GameModifier>
            <ModType>Resource</ModType>
            <Attribute>Mana</Attribute>
            <Value>3</Value>
            <PerTurn>1</PerTurn>
            <Provides>+3 Mana</Provides>
            <UpgradeComparisonID>1</UpgradeComparisonID>
        </GameModifier>
        <GameModifier>
            <ModType>ResourceMultiplier</ModType>
            <Attribute>Prestige</Attribute>
            <Value>1</Value>
            <PerTurn>1</PerTurn>
            <IsProductionQueueEmpty>1</IsProductionQueueEmpty>
            <Provides>+100% Growth when Producing Growth</Provides>
            <Calculate InternalName="Calc" ValueOwner="OwnerCity">
                <Expression><![CDATA[[Constructing_ProduceGrowth]]]></Expression>
            </Calculate>
            <Calculate InternalName="Value" ValueOwner="OwnerCity">
                <Expression><![CDATA[[Calc] + 1]]></Expression>
            </Calculate>
        </GameModifier>
        <GameModifier>
            <ModType>Resource</ModType>
            <Attribute>Fame</Attribute>
            <Value>1</Value>
            <PerTurn>1</PerTurn>
            <Provides>+1 Fame per Season</Provides>
        </GameModifier>
		<GameModifier>
            <ModType>Player</ModType>
            <Attribute>LimitedUnitCap</Attribute>
            <StrVal>Crusader</StrVal>
            <Value>3</Value>
            <Provides>Musters a Crusader</Provides>
        </GameModifier>
        <AIData AIPersonality="AI_General">
            <AIPriority>5</AIPriority>
            <AITag>Prestige</AITag>
        </AIData>
        <ArtDef>Art_Cathedral</ArtDef>
    </ImprovementType>
	
