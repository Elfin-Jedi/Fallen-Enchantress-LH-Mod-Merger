	<ImprovementType InternalName="Produce_X_Beer">
		<DisplayName>Produce Beer</DisplayName>
		<Description>Producing Beer increases the amount of Beer a city provides and is done instead of training units or constructing buildings.</Description>
		<Prereq>
			<Type>Tech</Type>
			<Attribute>Cooperation</Attribute>
		</Prereq>
		<LaborToBuild>-1</LaborToBuild>
		<GameModifier>
			<ModType>ConstructionResourceCost</ModType>
			<Attribute>Resource_X_Production</Attribute>
			<Value>-1</Value>
		</GameModifier>
		<GameModifier>
			<ModType>Resource</ModType>
			<Attribute>Resource_XL_Beer</Attribute>
			<PerTurn>1</PerTurn>
			<ApplyDuringConstruction>1</ApplyDuringConstruction>
			<Provides>+3 Beers per 2 Seasons</Provides>
			<UpgradeComparisonID>1</UpgradeComparisonID>
			<Value>1.5</Value>
		</GameModifier>
		<AIData AIPersonality="AI_General">
			<AIPriority>2</AIPriority>
			<AITag>Crystal</AITag>
		</AIData>
		<ArtDef>Produce_X_Beer_ArtDef</ArtDef>
	</ImprovementType>
