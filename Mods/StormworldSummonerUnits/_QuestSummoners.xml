					<IsOptional>0</IsOptional>
				</Objective>
				<Class>Success</Class>
				<Type>BattleFinished</Type>
				<TextData>IceElementals</TextData>
				<MoreTextData>Victory</MoreTextData>
			</QuestConditionDef>
