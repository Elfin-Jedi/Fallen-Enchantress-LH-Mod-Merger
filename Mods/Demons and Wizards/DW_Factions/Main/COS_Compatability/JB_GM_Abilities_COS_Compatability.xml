<?xml version="1.0"?>
<AbilityBonuses>
	
	<!-- Modify Race Bloods -->
	<AbilityBonus InternalName="Blood_AngelAbility">
        <AbilityBonusOption InternalName="Blood_Angel">
            <GameModifier>
                <ModType>Unit</ModType>
                <Attribute>BattleAutoCastSpell</Attribute>
                <StrVal>JB_AllBloodWorldSpellEffect</StrVal>
                <Value>100</Value>
                <Provides>This unit counts as having a Bloodline</Provides>
            </GameModifier>
        </AbilityBonusOption>
    </AbilityBonus>	
