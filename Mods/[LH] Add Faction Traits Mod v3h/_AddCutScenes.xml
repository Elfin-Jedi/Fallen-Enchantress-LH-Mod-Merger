	<Cutscene InternalName="Emer_ChooseSovereign">
		<Description>Emer's scene for the "choose sovereign" screen.</Description>
		<CutSceneObject InternalName="Background">
			<Type>Background</Type>
			<Image>SovereignCutscene_BG_Emer.png</Image>
		</CutSceneObject>

		<CutSceneObject InternalName="Foreground">
			<Type>Foreground</Type>
			<Image>SovereignCutscene_FG_Emer4.png</Image>
		</CutSceneObject>

    		<CutSceneObject InternalName="BackgroundParticle">
      			<Type>Particles</Type>
      			<Effect>C_ChooseSovereign_Karavox_1</Effect>
      			<Position>0.0,0.0,0.0</Position>
      			<Scale>1.0</Scale>
	    	</CutSceneObject>

		<CutSceneCamera InternalName="MainCamera">
			<Position>156.089005, 45.705002, -539.479004</Position>
			<LookAt>-5.471001, 38.029999, 8.221000</LookAt>
			<FOV>0.157080</FOV>
			<NearPlane>10</NearPlane>
			<FarPlane>2000</FarPlane>
		</CutSceneCamera>
		<CutSceneCamera InternalName="MountedCamera">
			<Position>-65,54,-80</Position>
			<LookAt>10,58,0</LookAt>
			<NearPlane>10</NearPlane>
			<FarPlane>2000</FarPlane>
		</CutSceneCamera>
    <DefaultLightAmbient>0,0,0</DefaultLightAmbient>
    <DefaultLightDiffuse>385,385,385</DefaultLightDiffuse>
    <DefaultLightDirection>0.066987,-0.250000,0.965926</DefaultLightDirection>
    <DefaultRimLightColor0>35,110,380</DefaultRimLightColor0>
    <DefaultRimLightDirection0>-0.965926,0.0,-0.258820</DefaultRimLightDirection0>
    <DefaultRimLightColor1>370,210,50</DefaultRimLightColor1>
    <DefaultRimLightDirection1>0.984808,0.0,-0.173648</DefaultRimLightDirection1>
  </Cutscene>
  
